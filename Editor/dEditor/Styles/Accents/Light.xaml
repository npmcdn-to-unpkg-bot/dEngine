<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:dEditor.Styles.Accents"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    <SolidColorBrush x:Key="BackgroundPrimary" Color="White" />
    <SolidColorBrush x:Key="BackgroundSecondary" Color="#EEE" />
    <SolidColorBrush x:Key="TextColourPrimary" Color="Black" /> 
    <SolidColorBrush x:Key="TextColourPrimaryInverse" Color="White" />
    <SolidColorBrush x:Key="TextColourSecondary" Color="Gray" />
    <SolidColorBrush x:Key="BorderPrimary" Color="#CCC" />
    <SolidColorBrush x:Key="AccentPrimary" Color="#4281F4" />
    <SolidColorBrush x:Key="HighlightInactiveBrush" Color="#9DBEFF" />
    <SolidColorBrush x:Key="MouseOverBrush" Color="#EEE" />
    <ImageSource x:Key="LogoSource">/dEditor;component/Content/Icons/logo-small.png</ImageSource>
</ResourceDictionary>